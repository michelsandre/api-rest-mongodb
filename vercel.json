{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }

    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "header": [
      {
        "source": "/(.*)",
        "header":[
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ]
  }